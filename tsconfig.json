{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["react", "react-dom", "@testing-library/jest-dom"],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowJs": true,
    "esModuleInterop": true
  }
}
